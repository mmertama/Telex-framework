include(FindPackageHandleStandardArgs)

find_path(usockets_INCLUDE_DIR
  NAMES libusockets.h
  PATHS @SOCKETS_SOURCES@
)

find_library(usockets_LIBRARY
  NAMES @SOCKETS_LIB_NAME@
  PATHS @SOCKETS_DIR@
)

find_package_handle_standard_args(usockets
  FOUND_VAR usockets_FOUND
  REQUIRED_VARS
    usockets_LIBRARY
    usockets_INCLUDE_DIR
  VERSION_VAR Foo_VERSION
)
